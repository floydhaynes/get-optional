{
  "name": "safeget",
  "version": "1.0.0",
  "description": "Small and typesafe object property navigation for JS & TS",
  "main": "lib/index.js",
  "scripts": {
    "build": "ts-node --project tsconfig.json build/index.ts",
    "debug": "nodemon --watch build -e ts --exec yarn build",
    "pretest": "yarn build && yarn --no-lockfile --cwd tests/typings/testbed",
    "test": "jest --verbose",
    "lint": "tslint --project ."
  },
  "keywords": [
    "safe",
    "navigation",
    "property"
  ],
  "author": "noppa",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.1",
    "flow-bin": "^0.77.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  }
}
