{
	"name": "get-optional",
	"version": "1.0.3",
	"description": "Small and typesafe object property navigation for TypeScript and Flow",
	"main": "lib/index.js",
	"module": "lib/index.mjs",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "yarn run build:runtime && yarn run build:types",
		"build:types": "ts-node --project tsconfig.json build/index.ts",
		"build:runtime": "tsc --project src/tsconfig.json",
		"prepublishOnly": "yarn build",
		"parseDocs": "ts-node --project tsconfig.json build/extractComments.ts",
		"debug": "nodemon --watch build -e ts --exec yarn build",
		"pretest": "yarn build && yarn --no-lockfile --cwd tests/typings/testbed",
		"test": "jest",
		"lint": "tslint --project .",
		"benchmark": "node tests/benchmark/run-all.js"
	},
	"keywords": [
		"safe",
		"navigation",
		"property",
		"typesafe",
		"typescript",
		"flow",
		"optional chaining",
		"get",
		"undefined",
		"null"
	],
	"author": "noppa",
	"license": "MIT",
	"devDependencies": {
		"@types/benchmark": "^1.0.31",
		"@types/jest": "^23.1.3",
		"@types/lodash": "^4.14.116",
		"@types/node": "^10.5.1",
		"babel-core": "^6.26.3",
		"babel-jest": "^23.4.2",
		"benchmark": "^2.1.4",
		"flow-bin": "^0.77.0",
		"jest": "^23.5.0",
		"lodash": "^4.17.10",
		"nodemon": "^1.18.3",
		"ts-jest": "^23.1.3",
		"ts-node": "^7.0.1",
		"tslint": "^5.10.0",
		"typescript": "^3.0.3"
	},
	"files": [
		"lib/index.js",
		"lib/index.d.ts",
		"lib/index.mjs",
		"lib/index.js.flow"
	],
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/noppa/get-optional.git"
	},
	"bus": {
		"url": "https://github.com/noppa/get-optional/issues"
	}
}
